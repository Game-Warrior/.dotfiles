{
   "background": {
      "scripts": [ "background.js" ]
   },
   "content_scripts": [ {
      "css": [ "styles.css" ],
      "matches": [ "*://old.reddit.com/*" ],
      "run_at": "document_start"
   } ],
   "description": "Ensure Reddit always loads the old design",
   "icons": {
      "128": "img/icon128.png",
      "48": "img/icon48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Old Reddit Redirect",
   "permissions": [ "webRequest", "webRequestBlocking", "*://reddit.com/*", "*://www.reddit.com/*", "*://np.reddit.com/*", "*://amp.reddit.com/*", "*://i.reddit.com/*" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.6.0"
}
